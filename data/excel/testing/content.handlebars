
{{! Add merged header title }}
{{#xlsxAdd "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row"}}
    <row height="30">
        <c r="A1" t="inlineStr" s="0">
            <is><t>PARTICIPANTS & INVITATION for cdcIPM launching</t></is>
        </c>
        <c r="B1" s="0"/>
        <c r="C1" s="0"/>
        <c r="D1" s="0"/>
        <c r="E1" s="0"/>
        <c r="F1" s="0"/>
        <c r="G1" s="0"/>
        <c r="H1" s="0"/>
        <c r="I1" s="0"/>
        <c r="J1" s="0"/>
        <c r="K1" s="0"/>
        <c r="L1" s="0"/>
    </row>
{{/xlsxAdd}}


{{! Add sub-headers }}
{{#xlsxAdd "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row"}}
    <row height="25">
        <c r="A2" t="inlineStr" s="1">
            <is><t>ល.រ.</t></is>
        </c>
