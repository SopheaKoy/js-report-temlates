        </c>
        <c r="C2" t="inlineStr" s="1">
            <is><t>អង្គភាព</t></is>
        </c>
        <c r="D2" t="inlineStr" s="1">
            <is><t>ចំនួន</t></is>
        </c>
        <c r="E2" t="inlineStr" s="1">
            <is><t>អម</t></is>
        </c>
        <c r="F2" t="inlineStr" s="1">
            <is><t>ជំពូក</t></is>
        </c>
        <c r="G2" t="inlineStr" s="1">
            <is><t>ថ្នាក់</t></is>
        </c>
        <c r="H2" t="inlineStr" s="1">
            <is><t>ជនបង្កោល</t></is>
        </c>
        <c r="I2" t="inlineStr" s="1">
            <is><t>លេខទូរសព្ទ</t></is>
        </c>
