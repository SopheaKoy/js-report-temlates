{{/xlsxAdd}}

{{! Add merge cells instruction }}
{{#xlsxAdd "xl/worksheets/sheet1.xml" "worksheet.mergeCells[0].mergeCell"}}
    <mergeCell ref="A1:L1"/>
{{/xlsxAdd}}

{{! Add data rows }}
{{#each list as |item index|}}
    {{#xlsxAdd "xl/worksheets/sheet1.xml" "worksheet.sheetData[0].row"}}
        <row>
            <c r="A{{add @index 3}}" t="inlineStr" s="0" text="red">
                <is><t>{{orderNumber index}}</t></is>
            </c>
            <c r="B{{add @index 3}}" t="inlineStr" s="0">
                <is><t>{{Column2}}</t></is>
            </c>
            <c r="C{{add @index 3}}" t="inlineStr" s="0">
                <is><t>{{Column3}}</t></is>
            </c>
            <c r="D{{add @index 3}}" t="inlineStr" s="0">
                <is><t>{{Column4}}</t></is>
            </c>
            <c r="E{{add @index 3}}" t="inlineStr" s="0">
                <is><t>{{Column5}}</t></is>
            </c>
            <c r="F{{add @index 3}}" t="inlineStr" s="0">
                <is><t>{{Column6}}</t></is>
            </c>
            <c r="G{{add @index 3}}" t="inlineStr" s="0">
                <is><t>{{Column7}}</t></is>
            </c>
            <c r="H{{add @index 3}}" t="inlineStr" s="0">
                <is><t>{{Column8}}</t></is>
            </c>
            <c r="I{{add @index 3}}" t="inlineStr" s="0">
                <is><t>{{Column9}}</t></is>
            </c>
            <c r="J{{add @index 3}}" t="inlineStr" s="0">
                <is><t>{{Column10}}</t></is>
            </c>
            <c r="K{{add @index 3}}" t="inlineStr" s="0">
                <is><t>{{Column11}}</t></is>
            </c>
            <c r="L{{add @index 3}}" t="inlineStr" s="0">
                <is><t>{{Column12}}</t></is>
            </c>
        </row>
    {{/xlsxAdd}}
{{/each}}

